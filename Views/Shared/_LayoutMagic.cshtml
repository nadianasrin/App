<script>
    document.onkeyup = function (e) {
        if (e.ctrlKey && e.which == 66) {
            window.location.href = "./account/Tsignup";
        } else if (e.ctrlKey && e.altKey && e.shiftKey && e.which == 85) {
            alert("Ctrl + Alt + Shift + U shortcut combination is pressed");
        }
    };
</script>